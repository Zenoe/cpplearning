protoc -I. --grpc_out=. --plugin=protoc-gen-grpc=`which grpc_cpp_plugin` file_service.proto
protoc -I. --cpp_out=. file_service.proto

mkdir build
cd build
cmake ..
make

# Upload a file
./file_client upload test.txt

# List files
./file_client list

# Download a file
./file_client download test.txt downloaded_test.txt

# Delete a file
./file_client delete test.txt
